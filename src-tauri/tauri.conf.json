{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "SpaceGT",
  "version": "0.1.0",
  "identifier": "com.spacegt.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3336",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "width": 1280,
        "height": 720,
        "minWidth": 845,
        "minHeight": 600,
        "fullscreen": false,
        "center": true,
        "resizable": true,
        "decorations": false,
        "visible": false
      },
      {
        "label": "splashscreen",
        "url": "splashscreen.html",
        "width": 1280,
        "height": 720,
        "minWidth": 845,
        "minHeight": 600,
        "fullscreen": false,
        "center": true,
        "resizable": false,
        "decorations": false,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
